{
  "name": "feelunique/slack",
  "description": "Access your Slack Team's API through PHP objects",
  "license": "proprietary",
  "keywords": [
    "slack",
    "slack api",
    "chat"
  ],
  "type": "library",
  "require": {
    "php": ">=5.4",
    "ext-curl": "*",
    "jms/serializer": "~1.0.0",
    "symfony/event-dispatcher": "2.*",
    "guzzlehttp/guzzle": "^6.0@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.4"
  },
  "autoload": {
    "psr-4": {
      "FeelUnique\\Slack\\": "src/FeelUnique/Slack/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
